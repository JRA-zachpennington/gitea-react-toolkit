{
  "name": "gitea-react-toolkit",
  "version": "0.6.3",
  "license": "MIT",
  "description": "A Gitea API React Toolkit Component Library",
  "homepage": "https://gitea-react-toolkit.netlify.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/unfoldingWord/gitea-react-toolkit"
  },
  "private": false,
  "dependencies": {
    "awesome-debounce-promise": "2.1.0",
    "axios": "0.19.0",
    "axios-cache-adapter": "2.4.1",
    "base-64": "0.1.0",
    "jszip": "3.2.1",
    "localforage": "1.7.3",
    "prop-types": "15.7.2",
    "utf8": "3.0.0"
  },
  "scripts": {
    "start": "docz dev",
    "docz:build": "docz build && npm run docz:netlify",
    "docz:netlify": "npx ./scripts/docz-netlify.js",
    "predeploy": "npm run docz:build",
    "prepublishOnly": "rm -fr ./dist & babel ./src --out-dir ./dist -s inline --extensions \".ts,.js\"",
    "cypress:run": "cypress run",
    "test:e2e": "NODE_ENV=test start-test 3000 cypress:run && nyc report --reporter=json-summary",
    "test:unit": "NODE_ENV=test jest ./src/core --coverage && cat ./coverage/lcov.info | coveralls",
    "test": "nyc --exclude-after-remap=false npm run test:e2e",
    "create-coverage-badge": "bash scripts/create-badge-json.sh"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.7.2",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.7.2",
    "@cypress/code-coverage": "1.10.1",
    "@istanbuljs/nyc-config-typescript": "0.1.3",
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2",
    "@types/base-64": "0.1.3",
    "@types/cypress": "1.1.3",
    "@types/jest": "24.0.22",
    "@types/utf8": "2.1.6",
    "@typescript-eslint/eslint-plugin": "2.7.0",
    "@typescript-eslint/parser": "2.7.0",
    "@unfoldingword/eslint-config": "1.0.11",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-plugin-istanbul": "5.2.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "coveralls": "3.0.7",
    "cypress": "3.6.1",
    "docz": "1.3.2",
    "docz-theme-default": "1.2.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-chai-friendly": "0.5.0",
    "eslint-plugin-cypress": "2.7.0",
    "eslint-plugin-mdx": "1.0.1",
    "eslint-plugin-react": "7.12.4",
    "istanbul-lib-coverage": "2.0.5",
    "jest": "24.9.0",
    "lorem-ipsum": "2.0.1",
    "markdown-translatable": "0.3.7",
    "moment": "2.24.0",
    "nyc": "14.1.1",
    "path": "latest",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "source-map-support": "0.5.16",
    "start-server-and-test": "1.10.6",
    "ts-jest": "24.1.0",
    "typescript": "3.7.2"
  },
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "resolutions": {
    "react": "16.11.0",
    "react-dom": "16.11.0"
  },
  "nyc": {
    "excludeAfterRemap": false,
    "exclude": [
      "**/*.spec.js"
    ],
    "include": [
      "**/components/**"
    ]
  }
}
